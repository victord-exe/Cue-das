def insertion_sort(array):

    cont = 0

    for i in range(1, len(array)):
        print(array)

        x = array[i]
        j = i

        while j > 0 and array[j - 1] > x:
            array[j] = array[j -1]
            j = j - 1
            cont +=1
        cont +=1

        array[j] = x

    return cont

array = [50, 30, 40, 10, 20]
